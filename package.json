{
    "name": "jandan-vscode",
    "displayName": "JanDan",
    "version": "0.1.1",
    "description": "A VS Code extension for JanDan.",
    "author": {
        "name": "Taipa Xu"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TaipaXu/jandan-vscode"
    },
    "bugs": "https://github.com/TaipaXu/jandan-vscode/issues",
    "publisher": "TaipaXu",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "jandan",
        "jiandan",
        "煎蛋",
        "娱乐"
    ],
    "license": "GPL-3.0",
    "icon": "static/images/icon.png",
    "activationEvents": [
        "onCommand:jandan.newsPrevious",
        "onCommand:jandan.newsNext",
        "onCommand:jandan.newsRefresh",
        "onCommand:jandan.picPrevious",
        "onCommand:jandan.picNext",
        "onCommand:jandan.picRefresh",
        "onCommand:jandan.ooxxPrevious",
        "onCommand:jandan.ooxxNext",
        "onCommand:jandan.ooxxRefresh",
        "onCommand:jandan.oo",
        "onCommand:jandan.xx"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "jandan.newsPrevious",
                "title": "上一页",
                "icon": "$(arrow-up)"
            },
            {
                "command": "jandan.newsNext",
                "title": "下一页",
                "icon": "$(arrow-down)"
            },
            {
                "command": "jandan.newsRefresh",
                "title": "刷新",
                "icon": "$(refresh)"
            },
            {
                "command": "jandan.picPrevious",
                "title": "上一页",
                "icon": "$(arrow-up)"
            },
            {
                "command": "jandan.picNext",
                "title": "下一页",
                "icon": "$(arrow-down)"
            },
            {
                "command": "jandan.picRefresh",
                "title": "刷新",
                "icon": "$(refresh)"
            },
            {
                "command": "jandan.ooxxPrevious",
                "title": "上一页",
                "icon": "$(arrow-up)"
            },
            {
                "command": "jandan.ooxxNext",
                "title": "下一页",
                "icon": "$(arrow-down)"
            },
            {
                "command": "jandan.ooxxRefresh",
                "title": "刷新",
                "icon": "$(refresh)"
            },
            {
                "command": "jandan.oo",
                "title": "oo"
            },
            {
                "command": "jandan.xx",
                "title": "xx"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "jandan.newsPrevious",
                    "when": "view == news",
                    "group": "navigation@0"
                },
                {
                    "command": "jandan.newsNext",
                    "when": "view == news",
                    "group": "navigation@1"
                },
                {
                    "command": "jandan.newsRefresh",
                    "when": "view == news",
                    "group": "navigation@2"
                },
                {
                    "command": "jandan.picPrevious",
                    "when": "view == pic",
                    "group": "navigation@0"
                },
                {
                    "command": "jandan.picNext",
                    "when": "view == pic",
                    "group": "navigation@1"
                },
                {
                    "command": "jandan.picRefresh",
                    "when": "view == pic",
                    "group": "navigation@2"
                },
                {
                    "command": "jandan.ooxxPrevious",
                    "when": "view == ooxx",
                    "group": "navigation@0"
                },
                {
                    "command": "jandan.ooxxNext",
                    "when": "view == ooxx",
                    "group": "navigation@1"
                },
                {
                    "command": "jandan.ooxxRefresh",
                    "when": "view == ooxx",
                    "group": "navigation@2"
                }
            ],
            "view/item/context": [
                {
                    "command": "jandan.oo",
                    "when": "view == pic"
                },
                {
                    "command": "jandan.xx",
                    "when": "view == pic"
                },
                {
                    "command": "jandan.oo",
                    "when": "view == ooxx"
                },
                {
                    "command": "jandan.xx",
                    "when": "view == ooxx"
                }
            ],
            "commandPalette": [
                {
                    "command": "jandan.newsPrevious",
                    "when": "never"
                },
                {
                    "command": "jandan.newsNext",
                    "when": "never"
                },
                {
                    "command": "jandan.newsRefresh",
                    "when": "never"
                },
                {
                    "command": "jandan.picPrevious",
                    "when": "never"
                },
                {
                    "command": "jandan.picNext",
                    "when": "never"
                },
                {
                    "command": "jandan.picRefresh",
                    "when": "never"
                },
                {
                    "command": "jandan.ooxxPrevious",
                    "when": "never"
                },
                {
                    "command": "jandan.ooxxNext",
                    "when": "never"
                },
                {
                    "command": "jandan.ooxxRefresh",
                    "when": "never"
                },
                {
                    "command": "jandan.oo",
                    "when": "never"
                },
                {
                    "command": "jandan.xx",
                    "when": "never"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "jandan",
                    "title": "JanDan",
                    "icon": "static/images/icon.svg"
                }
            ]
        },
        "views": {
            "jandan": [
                {
                    "id": "news",
                    "name": "新鲜事"
                },
                {
                    "id": "pic",
                    "name": "无聊图"
                },
                {
                    "id": "ooxx",
                    "name": "随手拍"
                },
                {
                    "id": "support",
                    "name": "支援煎蛋"
                }
            ]
        }
    },
    "scripts": {
        "build-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
        "dev": "npm run build-base -- --sourcemap --watch",
        "build": "npm run build-base -- --minify",
        "package": "vsce package --no-dependencies"
    },
    "devDependencies": {
        "@types/node": "^20.4.5",
        "@types/vscode": "^1.80.0",
        "esbuild": "^0.18.17",
        "vsce": "^2.15.0"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "form-data": "^4.0.0"
    }
}
